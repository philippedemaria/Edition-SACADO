\begin{enumerate}
\item $120$ minutes = $\ldots\ldots$ minutes 
\item $3$ heures en minutes = $\ldots\ldots$ minutes 
\item $240$ secondes en minutes = $\ldots\ldots$ minutes 
\end{enumerate}

